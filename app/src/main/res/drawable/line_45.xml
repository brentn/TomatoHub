<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="45"
    android:toDegrees="45"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="line"
        android:top="1dp">
        <stroke
            android:width="1dp"
            android:color="#000000" />
    </shape>
</rotate>